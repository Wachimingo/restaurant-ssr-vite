{
  "name": "vite-project",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "npx nodemon server.ts --dev",
    "build:server_stream": "vite build --emptyOutDir=false --ssr client/entry-server-stream.tsx --outDir server/public/",
    "build:server_prerender": "vite build --emptyOutDir=false --ssr client/entry-server-prerender.tsx --outDir server/public/",
    "inject": "npx ts-node ./prerender.ts",
    "generate": "vite build --ssrManifest  && npm run build:server_stream && npm run build:server_prerender && npm run inject",
    "build": "tsc && vite build",
    "prod": "npx nodemon server.ts --prod"
  },
  "dependencies": {
    "@fastify/autoload": "^5.2.0",
    "@fastify/static": "^6.5.0",
    "@reduxjs/toolkit": "^1.8.5",
    "dotenv": "^16.0.2",
    "fastify": "^4.5.3",
    "mongoose": "^6.5.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.34.0",
    "react-icons": "^4.4.0",
    "react-redux": "^8.0.2",
    "react-router-dom": "^6.3.0",
    "redux": "^4.2.0",
    "sharp": "^0.30.7"
  },
  "devDependencies": {
    "@types/node": "^18.7.13",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@vitejs/plugin-react": "^2.0.1",
    "nodemon": "^2.0.19",
    "ts-node": "^10.9.1",
    "typescript": "^4.6.4",
    "vite": "^3.0.7"
  }
}